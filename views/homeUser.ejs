<!DOCTYPE html>
<head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" type="text/css" href="/public/bootstrap/css/bootstrap.css">
        <link rel="stylesheet" type="text/css" href="/public/bootstrap/css/bootstrap-grid.min.css">
        <link rel="stylesheet" type="text/css" href="/public/bootstrap/css/footer.css">
        <link rel="stylesheet" type="text/css" href="/public/bootstrap/css/styling.css">
        <link rel="stylesheet" type="text/css" href="/public/bootstrap/css/modalScrollBar.css">
      
      
        
    </head>
    <body>
       
        <!-- navigation bar -->
        <nav class="navbar navbar-light navColoring" id="nav" >
            
            <button class="navbar-toggler" type="button" id="navButton" data-toggle="collapse" data-target = "#navInfo" aria-controls = "navInfo" aria-expanded="false" aria-label="Toggle navigation" >
                <span class="navbar navbar-toggler-icon"></span>

            </button>
            <a class="nav nav-item nav-link ml-1" id="signing-out-brand" href="/logout/user">Sign Out(<%= user.email  %>)  </a>
            
            <a class="navbar navbar-brand my-2" href="#"></a>
            
            <div class="collapse navbar-collapse" id="navInfo">
                <div class="navbar-nav my-4">
                    
                    <a class="nav nav-item nav-link ml-1 active" href="#">Home <span class="sr-only"></span></a>
                    <a class="nav nav-item nav-link ml-1" href="#">Newsfeed</a>
                    <a class="nav nav-item nav-link ml-1" href="#">Contact Us</a>
                    <a class="nav nav-item nav-link ml-1" href="#">About Us</a>
                    
                </div>

            </div>
                    
        </nav>
        <!-- creating the main content of the page    -->
        <div class="container" id="cards-container">
           
            <div class="row" id="row-cards">
                <div class="col-lg col-md col-sm" id="card-Start-Course" data-toggle="modal" data-target="#Start-CourseContent" >
                    <div class="card bg-transparent mx-3 my-4" id="Start-CourseCard" >
                        <div class="card-titlle">
                            <h6 class="display-4 words mx-3 my-4" id="Start-Course-title">Start Course<hr></h6>
                        </div>  
                        <table class="table table-hover" id ="users-table">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Course Name</th>
                                    <th scope="col">Source Format</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Results(%)</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% let count = 0 %> 
                                <% for(let user of course ){ %>
                                    <tr><td>  <%= count + 1  %>  </td><td>  <%= user.title  %>  </td> <td>Slides </td><td id="status">Not Attempted</td><td id="totalPercentage"></td></tr>
                                <% } %>     
                                
                            </tbody>
                            
                            
                          </table>
                    </div>
                    
                </div>
                
                
            </div>
            <!-- dashboard content -->

            <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="Start-CourseContent" aria-hidden="true" id="Start-CourseContent">
                <div class="modal-dialog" role="document">
                    <div class="modal-content bg-transparent">
                        <div class="modal-header">
                            <div class="modal-title">
                                <h3 class="display-4 words mx-3 my-4">Course</h3>    
                            </div>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            
                        </div>
                        <div class="">
                            <div class="container my-4">
                                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                                    <ol class="carousel-indicators">
                                      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                                      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                                      <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                                      <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
                                    
                                    </ol>
                                    <div class="carousel-inner">
                                      <div class="carousel-item active">
                                        <img class="d-block w-100" src="/public/courses/IMG-20210215-WA0008.jpg" alt="First slide">
                                      </div>
                                      <div class="carousel-item">
                                        <img class="d-block w-100" src="/public/courses/IMG-20210215-WA0009.jpg" alt="Second slide">
                                      </div>
                                      <div class="carousel-item">
                                        <img class="d-block w-100" src="/public/courses/IMG-20210215-WA0010.jpg" alt="Third slide">
                                      </div>
                                      <div class="carousel-item">
                                        <img class="d-block w-100" src="/public/courses/IMG-20210215-WA0011.jpg" alt="Second slide">
                                      </div>
                                      <div class="carousel-item">
                                        <img class="d-block w-100" src="/public/courses/IMG-20210215-WA0012.jpg" alt="Third slide">
                                      </div>
                                    </div>
                                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                                      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                                      <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                      <span class="sr-only">Next</span>
                                    </a>
                                  </div>
                            </div>
                            


                        </div>
                        <div class="modal-footer">
                            <button type="button" data-toggle="modal" data-target="#Start-QuizContent" id="start-Quiz" class="btn btn-outline-dark" data-dismiss="modal" >Start Quiz</button>
                            <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
                        </div>
                    </div>    
                        
                </div>
            </div>

            <!-- quiz content -->
            <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="dashboardContent" aria-hidden="true" id="dashboardContent">
                <div class="modal-dialog" role="document">
                    <div class="modal-content bg-transparent">
                        <div class="modal-header">
                            <div class="modal-title">
                                <h3 class="display-4 words mx-3 my-4">Updates</h3>    
                            </div>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            
                        </div>
                        <div class="modal-body">
                            <!-- inside the modal dashboard body -->
                            <div class="row" id="row-cards">
                                
                                <div class="col-lg col-md col-sm">
                                    <div class="card mx-3 my-4" id="add-user-container">
                                        <div class="card-titlle">
                                            <h6 class="words mx-3 my-4" id="Add-User-title">Add User<hr></h6>
                                        </div>  
                                        <div class="modal-body">
                                            <%- include('./partials/quizz')  %>
            
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-outline-dark"  data-dismiss="modal" >Start Quiz</button>
                                            <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>

                        </div>
                        
                    </div>    
                        
                </div>
            </div>
            

            <!-- view reports content -->

            <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="Start-QuizContent" aria-hidden="true" id="Start-QuizContent">
                
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <div class="modal-title">
                                <h3 class="display-4 words mx-3 my-4">Quiz</h3>    
                            </div>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                            
                        </div>
                        <div class="modal-body">
                            <div class="container-fluid">
                                <%- include('./partials/quizz')  %>
                            
                            </div>
                            
                            
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-outline-dark" id ="sendQuiz" data-dismiss="modal" >Submit</button>
                            <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
                        
                        </div>
                    </div>    
                        
                </div>
            </div>

        </div>
        <div class="container">
            <div class="" id="cards-container">
                <div class="row" id="row-cards">
                    <div class="col-lg col-md col-sm" id="card-add-user-" data-toggle="modal" data-target="#add-user-Content" >
                        <div class="card bg-transparent mx-3 my-4" id="add-user-Card" >
                            <div class="card-titlle">
                                <h6 class="display-4 words mx-3 my-4" id="add-user-title">View Progress<hr></h6>
                            </div>  
                            <p class="lead mx-3 my-4">
                                Lorem ipsum dolor, sit amet consectetur adipisicing elit. Aliquid odit nobis unde provident quae ducimus nulla, eveniet, laborum eligendi vitae quas cupiditate temporibus, illo consectetur animi necessitatibus suscipit earum. Quod!
                            </p>
                        </div>
                        
                    </div>
                    
                    
                </div>
                <!-- new user content -->
    
                <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="add-user-Content" aria-hidden="true" id="add-user-Content">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <div class="modal-title">
                                    <h3 class="display-4 words mx-3 my-4">Progress</h3>    
                                </div>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                                
                            </div>
                                <div class="modal-body">
                                    
                                </div>
                                <div class="modal-footer">
                                 
                                    <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
                                
                                </div>
                                
                            </form>
                                    
                            
                        </div>    
                            
                    </div>
                </div>
        </div>
        <%- include('./partials/footer') %>
       
        
        <script src="/public/bootstrap/js/bootstrap/jquery.js"></script>
        <script src="/public/bootstrap/js/bootstrap/bootstrap.min.js"></script>
        <script src="/public/bootstrap/js/quizzLogic.js"></script>
    </body>
</html>